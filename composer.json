{
    "name": "minkhantnaung/laravel-telegram-topic-logger",
    "description": "A Laravel logging channel for sending logs to Telegram with topic/thread support",
    "keywords": ["laravel", "logging", "telegram", "monolog", "topic", "thread"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MinKhantNaung",
            "email": "minkhantnaungkzw@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Minkhantnaung\\TelegramTopicLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minkhantnaung\\TelegramTopicLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minkhantnaung\\TelegramTopicLogger\\TelegramTopicLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

